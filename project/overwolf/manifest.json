{
  "$schema": "../../schema/owmanifest.json",
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "project",
    "version": "1.0.0",
    "author": "publisher",
    "icon": "IconMouseOver.png",
    "icon_gray": "IconMouseNormal.png",
    "launcher_icon": "Icon.ico",
    "dock_button_title": "project",
    "description": "what does it do?",
    "minimum-overwolf-version": "0.121.0.17"
  },
  "permissions": [
    "Extensions",
    "GameInfo",
    "Hotkeys",
    "Logging",
  ],
  "data": {
    "game_targeting": {
      "type": "dedicated",
      "game_ids": []
    },
    "launch_events": [{
      "event": "GameLaunch",
      "event_data": {
          "game_ids": [],
          "wait_for_stable_framerate": 30
      },
      "start_minimized": false
    }],
    "game_events": [],
    "protocol_override_domains": {
    },
    "user_agent": "Overwolf",
    "start_window": "main",
    "windows": {
      "main": {
        "file": "files/index.html",
        "transparent": true,
        "block_top_window_navigation": true,
        "popup_blocker": true,
        "clickthrough": false,
        "in_game_only": false,
        "show_in_taskbar": true,
        "disable_restore_animation": false,
        "grab_keyboard_focus": true,
        "mute": true,
        "min_size": {
          "width": 800, 
          "height": 500
        },
        "size": {
          "width": 1200, 
          "height": 700
        }
      }
    },
    "force_browser": "overwolf",
    "content_security_policy": "default-src 'self'",
    "externally_connectable": {
      "matches": [
      ]
    }
  }
}